#!/bin/bash

# Launch or focus on a given TUI identified by the passed in as the command.
# Use by commands like misty-launch-wifi to ensure there is only one wifi configuration screen open.

APP_ID="org.misty.$(basename "$1")"
LAUNCH_COMMAND="misty-launch-tui $@"

exec misty-launchfocus "$APP_ID" "$LAUNCH_COMMAND"
